{{#leaflet-map tagName="main" lat=lat lng=lng zoom=zoom onMoveend=(action "updateCenter") as |layers|}}
  {{layers.tile url="http://{s}.basemaps.cartocdn.com/light_all/{z}/{x}/{y}.png"}}
  {{#each model as |item|}}
    {{#layers.marker lat=item.lat lng=item.lng}}
      {{#popup-layer}}
        {{search-result item=item}}
      {{/popup-layer}}
    {{/layers.marker}}
  {{/each}}
{{/leaflet-map}}



